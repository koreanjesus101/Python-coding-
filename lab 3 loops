start_integer = int(input("Enter the starting integer: "))
end_integer = int(input("Enter the ending integer: "))

even_total = 0
odd_total = 0

for number in range(start_integer, end_integer + 1 ):
  if number % 2 == 0:
    even_total += number
  else:
    odd_total += number

print(f"The total of even integers from {start_integer} to {end_integer}:", even_total)
print(f"The toal of odd integers from {start_integer} to {end_integer}:", odd_total)


words = []
  word = input("Enter a word (or 'stop' to finish): ")
while word !="stop": 
  words.append(word)
  word = input(enter next wor or ('stop to finish):")

print(f"Least word was: {min(words)}")
print(f"Greatest word: {max(words)}")

